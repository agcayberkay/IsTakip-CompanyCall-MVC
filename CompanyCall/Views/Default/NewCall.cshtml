@model CompanyCall.Models.Entity.InCall

@{
    ViewBag.Title = "NewCall";
    Layout = "~/Views/Shared/CompanyLayout.cshtml";
}

<h2>New Call</h2>


<form method="post">

    <br />
    @Html.Label("Subject")
    @Html.TextBoxFor(x => x.Subjects, new { @class = "form-control" })
    <br />
    <br />
    @Html.Label("Description")
    @Html.TextAreaFor(x => x.Descriptions, 10, 4, new { @class = "form-control" })
    <br />
    <br />
    <button class="btn btn-primary">Add Call</button>

</form>

